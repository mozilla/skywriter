+        this.mode.autoOutdent("start", doc, 2);
+        assertEquals("  }", doc.getLine(2));
+    },
+
+    "test: no auto outdent if no matching brace is found" : function() {
+        var doc = new Document(["  function foo()", "    bla", "    }"]);
+        this.mode.autoOutdent("start", doc, 2);
+        assertEquals("    }", doc.getLine(2));
+    }
+
+});

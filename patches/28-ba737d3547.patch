+        assertFalse(this.mode.checkOutdent("start", "", "}"));
+        assertFalse(this.mode.checkOutdent("start", "   ", "a }"));
+        assertFalse(this.mode.checkOutdent("start", "   }", "}"));
+    },
+
+    "test: auto outdent should indent the line with the same indent as the line with the matching opening brace" : function() {
